@model SAT.HR.Models.BonusCalculatorProcessViewModel

<div class="card">
    <div class="card-header">
        <h4 class="card-title"><span> ประมวลผล</span></h4>
    </div>
    <div class="row justify-content-center">
        <div class="col-sm-6" style="margin-top:-30px">
            <div class="row">
                <label class="col-sm-3 col-form-label m-t-20">ปีบัญชี</label>
                <div class="col-sm-3">
                    <div class="form-group bmd-form-group">
                        @Html.TextBoxFor(m => m.Year, new { @class = "form-control text-left", type = "number" })
                    </div>
                </div>
            </div>
            <div class="row m-b-30">
                <label class="col-sm-3 col-form-label m-t-20">อัตรา (เท่า)</label>
                <div class="col-sm-3">
                    <div class="form-group bmd-form-group">
                        @Html.TextBoxFor(m => m.UpStep, new { @class = "form-control text-left", type = "number" })
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
